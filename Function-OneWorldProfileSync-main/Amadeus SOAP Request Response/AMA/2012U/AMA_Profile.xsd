      <xs:element name="LocationCountryName" type="CountryNameType" minOccurs="0" />
      <xs:element name="TPA_Extensions" type="TPA_ExtensionsCompanyType" minOccurs="0" />
      <!-- added by vdoleans for slim samoa -->
