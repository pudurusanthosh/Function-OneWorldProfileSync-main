      <xs:element name="LoyaltyProgram" type="LoyaltyProgramType" minOccurs="0" maxOccurs="100" />
      <xs:element name="ExternalLoyaltyProgram" type="LoyaltyProgramType" minOccurs="0" maxOccurs="99" />
      <xs:element name="ContactPerson" type="ContactPersonType" minOccurs="0" maxOccurs="100" />
      <xs:element name="PersonalId" type="StringLength0to8" minOccurs="0" />
